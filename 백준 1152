#include <stdio.h>
#include <string.h>
int main(void)
{
        char string[1000000];
        int count=0,n,i;
        scanf("%[^\n]",string);
        n=strlen(string);

        for(i=0;i<n;i++)
        {
                if(string[i]!=' ')
                {
                        count++;
                }
                while(string[i]!=' ')
                {
                        i++;

                }

        }
        printf("%d",count);
        return 0;


}
